<?xml version="1.0" encoding="utf-8"?>
<manifest package="com.bingo.king"
          xmlns:android="http://schemas.android.com/apk/res/android">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name=".viewevent.ViewEventActivity">
        </activity>

        <service android:name=".servicedemo.MyService"/>
        <service android:name=".servicedemo.AidlService"/>

        <activity android:name=".servicedemo.ServiceActivity">
        </activity>
        <activity android:name=".servicedemo.AidlActivity">
        </activity>
        <activity android:name=".handler.HandlerActivity">
        </activity>
        <activity android:name=".devolopeskill.SkillActivity">
        </activity>
    </application>

</manifest>